#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kenny/dev/supabase-dom/node_modules/.pnpm/supabase@1.207.9/node_modules/supabase/bin/node_modules:/Users/kenny/dev/supabase-dom/node_modules/.pnpm/supabase@1.207.9/node_modules/supabase/node_modules:/Users/kenny/dev/supabase-dom/node_modules/.pnpm/supabase@1.207.9/node_modules:/Users/kenny/dev/supabase-dom/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kenny/dev/supabase-dom/node_modules/.pnpm/supabase@1.207.9/node_modules/supabase/bin/node_modules:/Users/kenny/dev/supabase-dom/node_modules/.pnpm/supabase@1.207.9/node_modules/supabase/node_modules:/Users/kenny/dev/supabase-dom/node_modules/.pnpm/supabase@1.207.9/node_modules:/Users/kenny/dev/supabase-dom/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../supabase/bin/supabase"   "$@"
exit $?
